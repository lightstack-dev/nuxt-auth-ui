{
  "name": "@lightstack/nuxt-auth-ui",
  "version": "0.1.0",
  "description": "Beautiful, production-ready authentication UI for Nuxt, powered by Logto",
  "repository": "lightstack-dev/nuxt-auth-ui",
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "docs",
    "playground"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:docs": "nuxi dev docs",
    "dev:build": "nuxi build playground",
    "prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "docs:build": "nuxi build docs",
    "docs:generate": "nuxi generate docs",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:module": "eslint src/",
    "lint:docs": "eslint docs/",
    "lint:playground": "eslint playground/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3",
    "@nuxt/ui": "^4.0.0-alpha.0",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "@logto/nuxt": "*",
    "nuxt": "^4.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.34.0",
    "nuxt": "^4.0.3",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  }
}
